{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "typescript": {
          "tsconfig": "tsconfig.json"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/start-ai",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
